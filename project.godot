; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Idle Internet"
config/description="Idle Internet is an Internet-themed clicker game where you must send packets to upgrade and expand your network."
run/main_scene="res://scenes/game.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://icons/router.png"

[autoload]

Globals="*res://globals.gd"

[display]

window/size/viewport_width=400
window/size/resizable=false

[gui]

theme/custom_font="res://Cybersomething.ttf"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
